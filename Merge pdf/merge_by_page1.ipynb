        "from PyPDF2 import PdfMerger\n",
        "\n",
        "pdfs = ['/content/1.pdf', '/content/2.pdf', '/content/3.pdf', '/content/4.pdf']\n",
        "\n",
        "merger = PdfMerger()\n",
        "\n",
        "for pdf in pdfs:\n",
        "  merger.append(pdf, pages=(0,2))\n",
        "merger.write(\"merge2.pdf\")\n",
        "merger.close()\n"
